{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2|^8.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "dev-extensions/kernel": "*@dev",
        "dev/api": "*@dev",
        "dev/data-synchronize": "*@dev",
        "dev/get-started": "*@dev",
        "dev/installer": "*@dev",
        "dev/menu": "*@dev",
        "dev/optimize": "*@dev",
        "dev/page": "*@dev",
        "dev/dev": "*@dev",
        "dev/plugin-management": "*@dev",
        "dev/revision": "*@dev",
        "dev/seo-helper": "*@dev",
        "dev/shortcode": "*@dev",
        "dev/sitemap": "*@dev",
        "dev/slug": "*@dev",
        "dev/theme": "*@dev",
        "dev/widget": "*@dev",
        "doctrine/dbal": "^4.2",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "12.34.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10",
        "predis/predis": "^3.0",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "dev/dev-tool": "*@dev",
        "dev/git-commit-checker": "*@dev",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Aws\\Script\\Composer\\Composer::removeUnusedServices",
            "Google\\Task\\Composer::cleanup",
            "@php artisan cms:publish:assets"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run prod"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "./dev/plugins/*/composer.json",
                "./dev/ui/*/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": false,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        },
        "aws/aws-sdk-php": [
            "S3",
            "Ses",
            "Translate"
        ],
        "google/apiclient-services": [
            "Drive"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./dev/core"
        },
        {
            "type": "path",
            "url": "./dev/libs/*"
        },
        {
            "type": "path",
            "url": "./dev-extensions/kernel",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./dev-extensions/libs/*",
            "options": {
                "symlink": true
            }
        }
    ]
}